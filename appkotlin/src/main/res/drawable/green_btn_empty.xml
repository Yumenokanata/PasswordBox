<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="4dp" />
            <solid android:color="@color/color_wh"/>
            <stroke android:width="1dp" android:color="@color/color_gr5" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="4dp" />
            <solid android:color="@color/color_gr6"/>
            <stroke android:width="1dp" android:color="@color/color_ge1"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="4dp" />
            <solid android:color="@color/color_wh"/>
            <stroke android:width="1dp" android:color="@color/color_ge1"/>
        </shape>
    </item>
</selector>